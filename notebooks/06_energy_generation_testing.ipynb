    "def load_energy_models(models_dir=\"energy_models\"):\n",
    "    \"\"\"Load all trained energy generation models and preprocessing components.\"\"\"\n",
    "    print(\"üîÑ Loading trained energy generation models...\")\n",
    "    \n",
    "    models_path = Path(models_dir)\n",
    "    if not models_path.exists():\n",
    "        raise FileNotFoundError(f\"Models directory '{models_dir}' not found. Please run training notebook first.\")\n",
    "    \n",
    "    # Load models\n",
    "    models = {}\n",
    "    model_files = {\n",
    "        'random_forest': 'energy_model_random_forest.pkl',\n",
    "        'gradient_boosting': 'energy_model_gradient_boosting.pkl',\n",
    "        'linear_regression': 'energy_model_linear_regression.pkl',\n",
    "        'ridge_regression': 'energy_model_ridge_regression.pkl'\n",
    "    }\n",
    "    \n",
    "    for model_name, filename in model_files.items():\n",
    "        model_path = models_path / filename\n",
    "        if model_path.exists():\n",
    "            models[model_name] = joblib.load(model_path)\n",
    "            print(f\"‚úÖ {model_name} energy model loaded\")\n",
    "        else:\n",
    "            print(f\"‚ö†Ô∏è  {model_name} energy model not found\")\n",
    "    \n",
    "    # Load scaler\n",
    "    scaler_path = models_path / \"energy_scaler.pkl\"\n",
    "    scaler = None\n",
    "    if scaler_path.exists():\n",
    "        scaler = joblib.load(scaler_path)\n",
    "        print(\"‚úÖ Energy scaler loaded\")\n",
    "    else:\n",
    "        print(\"‚ùå Energy scaler not found\")\n",
    "    \n",
    "    # Load feature information\n",
    "    feature_info_path = models_path / \"energy_feature_info.json\"\n",
    "    feature_info = None\n",
    "    if feature_info_path.exists():\n",
    "        with open(feature_info_path, 'r') as f:\n",
    "            feature_info = json.load(f)\n",
    "        print(\"‚úÖ Energy feature info loaded\")\n",
    "    \n",
    "    if not models:\n",
    "        raise FileNotFoundError(\"No trained energy models found! Please run training notebook first.\")\n",
    "    \n",
    "    return models, scaler, feature_info\n",
    "\n",
    "# Load models\n",
    "models, scaler, feature_info = load_energy_models()\n",
    "\n",
    "print(f\"\\nüìä Loaded Models: {list(models.keys())}\")\n",
    "if feature_info:\n",
    "    print(f\"üéØ Target Variable: {feature_info['target']}\")\n",
    "    print(f\"üìã Number of Features: {feature_info['n_features']}\")\n",
    "    if 'best_model' in feature_info:\n",
    "        print(f\"üèÜ Best Model: {feature_info['best_model']}\")"
   ]
