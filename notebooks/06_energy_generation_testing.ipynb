   "source": [
    "def create_sample_test_data():\n",
    "    \"\"\"Create sample test data for demonstration.\"\"\"\n",
    "    print(\"üîÑ Creating sample test dataset...\")\n",
    "    \n",
    "    # Generate 1 week of hourly data (different from training)\n",
    "    start_date = datetime(2023, 9, 1)  # Different time period\n",
    "    dates = [start_date + timedelta(hours=i) for i in range(24*7)]\n",
    "    \n",
    "    np.random.seed(123)  # Different seed for different patterns\n",
    "    n_samples = len(dates)\n",
    "    \n",
    "    # Generate realistic but different solar patterns\n",
    "    hours = np.array([d.hour for d in dates])\n",
    "    days = np.array([d.timetuple().tm_yday for d in dates])\n",
    "    \n",
    "    # Different irradiance pattern (slightly different location/weather)\n",
    "    irradiance = np.maximum(0, \n",
    "        750 * np.sin(np.pi * (hours - 7) / 11) *  # Different peak time\n",
    "        (1 + 0.15 * np.sin(2 * np.pi * days / 365)) + \n",
    "        np.random.normal(0, 120, n_samples)  # Different noise level\n",
    "    )\n",
    "    \n",
    "    # Different temperature pattern\n",
    "    temperature = (22 + 6 * np.sin(2 * np.pi * days / 365) + \n",
    "                  4 * np.sin(np.pi * (hours - 7) / 11) + \n",
    "                  np.random.normal(0, 2.5, n_samples))\n",
    "    \n",
    "    # Power generation with different efficiency\n",
    "    power = np.maximum(0, \n",
    "        irradiance * 2.2 + temperature * 15 +  # Different coefficients\n",
    "        np.random.normal(0, 180, n_samples)\n",
    "    )\n",
    "    \n",
    "    # Convert to energy generation\n",
    "    generation_kwh = power / 1000  # Convert W to kWh for 1-hour intervals\n",
    "    \n",
    "    # Additional weather variables\n",
    "    humidity = np.random.normal(55, 12, n_samples)\n",
    "    wind_speed = np.random.exponential(4.5, n_samples)\n",
    "    rainfall = np.random.exponential(0.3, n_samples)\n",
    "    pressure = np.random.normal(1015, 8, n_samples)\n",
    "    \n",
    "    # Create DataFrame\n",
    "    test_data = pd.DataFrame({\n",
    "        'Time': dates,\n",
    "        'generation(kWh)': generation_kwh,\n",
    "        'power(W)': power,\n",
    "        'Irradiance': irradiance,\n",
    "        'Temperature': temperature,\n",
    "        'RelativeHumidity': humidity,\n",
    "        'WindSpeed': wind_speed,\n",
    "        'Rainfall': rainfall,\n",
    "        'SeaLevelPressure': pressure\n",
    "    })\n",
    "    \n",
    "    print(f\"‚úÖ Sample test dataset created: {test_data.shape}\")\n",
    "    return test_data\n",
    "\n",
    "# Option 1: Create sample data for demonstration\n",
    "test_data = create_sample_test_data()\n",
    "\n",
    "# Option 2: Load your own data (uncomment and modify path)\n",
    "# test_data = pd.read_csv('path/to/your/test_data.csv')\n",
    "# test_data['Time'] = pd.to_datetime(test_data['Time'])\n",
    "\n",
    "print(f\"üìä Test dataset shape: {test_data.shape}\")\n",
    "print(f\"üìÖ Time range: {test_data['Time'].min()} to {test_data['Time'].max()}\")\n",
    "print(f\"‚ö° Energy range: {test_data['generation(kWh)'].min():.3f} to {test_data['generation(kWh)'].max():.3f} kWh\")\n",
    "\n",
    "# Display first few rows\n",
    "test_data.head()"
   ]
