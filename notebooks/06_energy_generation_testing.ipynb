    "## 5. Model Testing and Evaluation"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def test_energy_models(models, scaler, X_test, y_test):\n",
    "    \"\"\"Test all energy generation models on test data.\"\"\"\n",
    "    print(\"üîÑ Testing energy generation models on new data...\")\n",
    "    \n",
    "    results = {}\n",
    "    \n",
    "    for model_name, model in models.items():\n",
    "        print(f\"\\nüîÑ Testing {model_name}...\")\n",
    "        \n",
    "        try:\n",
    "            # Scale features if needed\n",
    "            if model_name in ['linear_regression', 'ridge_regression'] and scaler:\n",
    "                X_scaled = scaler.transform(X_test)\n",
    "                y_pred = model.predict(X_scaled)\n",
    "            else:\n",
    "                y_pred = model.predict(X_test)\n",
    "            \n",
    "            # Calculate metrics\n",
    "            test_results = {\n",
    "                'model_name': model_name,\n",
    "                'predictions': y_pred,\n",
    "                'n_samples': len(y_pred)\n",
    "            }\n",
    "            \n",
    "            if y_test is not None:\n",
    "                # Remove NaN values\n",
    "                mask = ~(np.isnan(y_test) | np.isnan(y_pred))\n",
    "                y_clean = y_test[mask]\n",
    "                y_pred_clean = y_pred[mask]\n",
    "                \n",
    "                if len(y_clean) > 0:\n",
    "                    rmse = np.sqrt(mean_squared_error(y_clean, y_pred_clean))\n",
    "                    mae = mean_absolute_error(y_clean, y_pred_clean)\n",
    "                    r2 = r2_score(y_clean, y_pred_clean)\n",
    "                    \n",
    "                    # Energy-specific metrics\n",
    "                    mape = np.mean(np.abs((y_clean - y_pred_clean) / (y_clean + 1e-6))) * 100\n",
    "                    total_actual = y_clean.sum()\n",
    "                    total_predicted = y_pred_clean.sum()\n",
    "                    total_error_pct = abs(total_actual - total_predicted) / total_actual * 100\n",
    "                    \n",
    "                    # Daily energy accuracy\n",
    "                    if len(y_clean) >= 24:\n",
    "                        daily_actual = y_clean.groupby(y_clean.index // 24).sum()\n",
    "                        daily_predicted = pd.Series(y_pred_clean).groupby(pd.Series(y_pred_clean).index // 24).sum()\n",
    "                        daily_rmse = np.sqrt(mean_squared_error(daily_actual, daily_predicted))\n",
    "                        daily_r2 = r2_score(daily_actual, daily_predicted)\n",
    "                    else:\n",
    "                        daily_rmse = rmse\n",
    "                        daily_r2 = r2\n",
    "                    \n",
    "                    test_results.update({\n",
    "                        'rmse': rmse,\n",
    "                        'mae': mae,\n",
    "                        'r2': r2,\n",
    "                        'mape': mape,\n",
    "                        'total_error_pct': total_error_pct,\n",
    "                        'daily_rmse': daily_rmse,\n",
    "                        'daily_r2': daily_r2,\n",
    "                        'actual': y_clean,\n",
    "                        'predicted': y_pred_clean\n",
    "                    })\n",
    "                    \n",
    "                    print(f\"‚úÖ {model_name} Energy Generation Test Results:\")\n",
    "                    print(f\"   RMSE: {rmse:.4f} kWh\")\n",
    "                    print(f\"   MAE: {mae:.4f} kWh\")\n",
    "                    print(f\"   R¬≤: {r2:.4f} ({r2*100:.2f}% variance explained)\")\n",
    "                    print(f\"   Total Energy Error: {total_error_pct:.2f}%\")\n",
    "                    print(f\"   Daily R¬≤: {daily_r2:.4f}\")\n",
    "                else:\n",
    "                    print(\"‚ö†Ô∏è  No valid data points for metric calculation\")\n",
    "            \n",
    "            results[model_name] = test_results\n",
    "            \n",
    "        except Exception as e:\n",
    "            print(f\"‚ùå Error testing {model_name}: {e}\")\n",
    "    \n",
    "    return results\n",
    "\n",
    "# Test all models\n",
    "test_results = test_energy_models(models, scaler, X_test, y_test)\n",
    "\n",
    "# Find best performing model\n",
    "results_with_metrics = {k: v for k, v in test_results.items() if 'rmse' in v}\n",
    "\n",
    "if results_with_metrics:\n",
    "    best_test_model = min(results_with_metrics.keys(), key=lambda x: results_with_metrics[x]['rmse'])\n",
    "    best_test_results = results_with_metrics[best_test_model]\n",
    "    \n",
    "    print(f\"\\nüèÜ Best Test Model: {best_test_model}\")\n",
    "    print(f\"üìä Best Test Performance:\")\n",
    "    print(f\"   ‚Ä¢ RMSE: {best_test_results['rmse']:.4f} kWh\")\n",
    "    print(f\"   ‚Ä¢ R¬≤: {best_test_results['r2']:.4f} ({best_test_results['r2']*100:.2f}% variance explained)\")\n",
    "    print(f\"   ‚Ä¢ Total Energy Error: {best_test_results['total_error_pct']:.2f}%\")"
   ]
